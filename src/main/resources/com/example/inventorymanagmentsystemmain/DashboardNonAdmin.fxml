            </Tab>
            <Tab fx:id="viewGoods">
              <content>
                  <fx:include source="view_goods.fxml" />
              </content>
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@storage.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Tab>
            <Tab fx:id="viewBillsTab">
              <content>
                <fx:include source="bills_dashboard.fxml" />
